#include <iostream>
#include <string>

#define MAX 160
#define LEN 100
#define NUMBER 10000
using namespace std;

int main()
{
	string input;
	int input_num[NUMBER];
	cin >> input;
	int len = input.size();
	for (int i = len - 1; i != -1; i--)
	{
		input_num[i] = input[i] - '0';
	}
	for (int i = 0; i < len; i++)
		cout << input_num[i];

	/*int Num[MAX][LEN] = { 0 };
