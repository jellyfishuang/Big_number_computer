		{
			Num[i][j] += Num[i - 1][j] * i;
			if (Num[i][j] >= 10)
			{
				Num[i][j + 1] += Num[i][j] / 10;
				Num[i][j] %= 10;
			}
		}
	}
	*/
	system("pause");
	return 0;
}


