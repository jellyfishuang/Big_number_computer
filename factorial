// ConsoleApplication1.cpp : 定義主控台應用程式的進入點。
//

#include <iostream>
#include <string>

using namespace std;
#define LEN 56000
#define MAX 6666
int Num[MAX + 1][LEN];

class fac
{
public:
	void process();
	void Print(int n);
};

void fac::process()
{
	int i, j;
	for (i = 0; i<MAX; i++)
		for (j = 0; j<LEN; j++)
			Num[i][j] = 0;
	Num[0][0] = 1;
	Num[1][0] = 1;
	for (i = 2; i <= MAX; i++)
		for (j = 0; j<LEN; j++) 
		{
			Num[i][j] += Num[i - 1][j] * i;
			if (Num[i][j] >= 1000) 
			{
				Num[i][j + 1] += Num[i][j] / 1000;
				Num[i][j] %= 1000;
			}
		}
}
void fac::Print(int n)
{
	int i;
	cout << n << "!=";
	for (i = LEN - 1; i>0; i--)
		if (Num[n][i] != 0) 
			break;
	for (; i >= 0; i--)
		cout << Num[n][i];
	cout << endl;
}

int main()
{
	fac number;
	int input;
	while (cin >> input)
	{
		number.process();
		number.Print(input);
	}


    return 0;
}

